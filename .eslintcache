div>Main picture</div>\r\n            <div>\r\n                <img src={props.userProfile.photos.large}/>\r\n                ava + description\r\n            </div>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default ProfileInfo;","I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\components\\Profile\\MyPosts\\Post\\Post.jsx",["141"],"import React from 'react';\r\nimport s from './Post.module.css'\r\n\r\n\r\nconst Post = (props) =>{\r\n    return(\r\n        <div className={s.item}>\r\n            <img src={props.image}/>\r\n            {props.text}\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default Post;","I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\components\\Dialogs\\Message\\Message.jsx",[],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\components\\Dialogs\\Dialog\\Dialog.jsx",[],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\redux\\state.js",[],["142","143"],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\redux\\profile-reducer.js",[],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\redux\\dialogs-reduce.js",[],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\redux\\redux-store.js",[],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\components\\Profile\\MyPosts\\MyPostsContainer.jsx",["144"],"import React from 'react';\r\nimport { connect } from 'react-redux';\r\nimport { addPostActionCreator, updateNewPostTextActionCreator } from '../../../redux/profile-reducer';\r\nimport MyPosts from './MyPosts';\r\n\r\n\r\nlet mapToStateProps = (state) =>{\r\n    return {\r\n        postData : state.profilePage.postData,\r\n        newPostText : state.profilePage.newPostText\r\n    }\r\n}\r\n\r\nlet mapToDispatchProps = (dispatch) =>{\r\n    return {\r\n        addPost : () => {\r\n            let action = addPostActionCreator();\r\n            dispatch(action);\r\n        },\r\n        changeNewPostText : (inputText) => {\r\n            let action = updateNewPostTextActionCreator(inputText);\r\n            dispatch(action);\r\n        }\r\n    }\r\n}\r\n\r\nconst MyPostsContainer = connect(mapToStateProps,mapToDispatchProps)(MyPosts);\r\n\r\nexport default MyPostsContainer;","I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\components\\Dialogs\\DialogsContainer.jsx",["145","146","147","148"],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\StoreContext.js",[],"I:\\Zahar\\React\\react-social-network\\react-social-network\\src\\redux\\users-reducer.js",["149","150","151"],"import {userAPI} from '../../src/api/api';\r\n\r\nlet FOLLOW = 'FOLLOW';\r\nlet UNFOLLOW = 'UNFOLLOW';\r\nlet SET_USERS = 'SET-USERS';\r\nlet SELECT_PAGE = 'SET-SELECT-PAGE';\r\nlet SET_USER_COUNT = 'SET-USER-COUNT';\r\nlet TOOGLE_IS_FETCHING = 'TOOGLE-IS-FETCHING';\r\nlet TOOGLE_FOLLOWING_IN_PROGRESS = 'TOOGLE-FOLLOWING-IN-PROGRESS';\r\n\r\nlet initialState = {\r\n    users : [\r\n        {id:\"1\", photos:{small:\"https://uznayvse.ru/images/stories/uzn_1379065615.jpg\"},\r\n         name:\"Sasha\", followed:true, status:'I Sasha', location:{city:'Minsk', country:'Belarus'}},\r\n        {id:\"2\", photos:{small:\"https://n1s2.starhit.ru/62/01/ee/6201ee27060ea9a3ed9273c7f3c3c1f6/480x497_0_05b7fbf09ee1e66007c5dd69db2d2706@480x497_0xac120003_7472114441591986839.jpg\"}, \r\n        name:\"Pasha\", followed:false, status:'I Pasha', location:{city:'Borisov', country:'Belarus'}},\r\n        {id:\"3\", photos:{small:\"https://the-most-beautiful.ru/sites/default/files/5125fc85d3c0d.jpg\"}, \r\n        name:\"Masha\", followed:false, status:'I Masha', location:{city:'Slonim', country:'Belarus'}},\r\n        {id:\"4\", photos:{small:\"https://tntmusic.ru/media/content/article/2020-08-07_17-40-42__1d94b6b6-d8d5-11ea-9963-e7bb315c2d29.jpg\"}, \r\n        name:\"Karina\", followed:true, status:'I Karina', location:{city:'Minsk', country:'Belarus'}},\r\n    ],\r\n    pageSize : 5,\r\n    userCount : 0,\r\n    selectedPage : 1,\r\n    isFetching : false,\r\n    followingInProgress:[]\r\n};\r\n\r\nconst usersReducer = (state = initialState,action) => {\r\n    switch(action.type){\r\n        case FOLLOW:{\r\n            return {\r\n                ...state,\r\n                users : state.users.map(el => {\r\n                    if(el.id === action.userId){\r\n                        el.followed = true;\r\n                    }\r\n                    return el;\r\n                })\r\n            };\r\n        }\r\n        case UNFOLLOW:{\r\n            return {\r\n                ...state,\r\n                users : state.users.map(el => {\r\n                    if(el.id === action.userId){\r\n                        el.followed = false;\r\n                    }\r\n                    return el;\r\n                })\r\n            };\r\n        }\r\n        case SET_USERS:{\r\n            return {\r\n                ...state,\r\n                //users : [...state.users, ...action.users]\r\n                users : [...action.users]\r\n            };\r\n        }\r\n        case SELECT_PAGE:{\r\n            return {\r\n                ...state,\r\n                selectedPage : action.page\r\n            };\r\n        }\r\n        case SET_USER_COUNT:{\r\n            return {\r\n                ...state,\r\n                userCount : action.userCount\r\n            };\r\n        }\r\n        case TOOGLE_IS_FETCHING:{\r\n            return {\r\n                ...state,\r\n                isFetching : action.isFetching\r\n            };\r\n        }\r\n        case TOOGLE_FOLLOWING_IN_PROGRESS:{\r\n            return {\r\n                ...state,\r\n                followingInProgress : action.isFetching \r\n                ? [...state.followingInProgress,action.userId]\r\n                : state.followingInProgress.filter(id => id != action.userId)\r\n            };\r\n        }\r\n        default:{\r\n            return state;\r\n        }\r\n      \r\n    }\r\n}\r\n\r\nexport const follow = (userId) =>{\r\n    return {type:FOLLOW,userId};\r\n}\r\n\r\nexport const unfollow = (userId) =>{\r\n    return {type:UNFOLLOW,userId};\r\n}\r\n\r\nexport const setUsers = (users) =>{\r\n    console.log(users)\r\n    return {type:SET_USERS,users};\r\n}\r\n\r\nexport const selectPage = (page) =>{\r\n    return {type:SELECT_PAGE,page};\r\n}\r\n\r\nexport const setUserCount = (userCount) =>{\r\n    return {type:SET_USER_COUNT,userCount};\r\n}\r\n\r\nexport const toogleIsFetching = (isFetching) =>{\r\n    return {type:TOOGLE_IS_FETCHING,isFetching};\r\n}\r\n\r\nexport const toogleFollowingInProgress = (isFetching,userId) =>{\r\n    return {type:TOOGLE_FOLLOWING_IN_PROGRESS,isFetching,userId};\r\n}\r\n\r\n\r\nexport const getUserThunkCreator = () => {\r\n    return (dispatch) => {\r\n            userAPI.getAllUsers().then(data => {\r\n                dispatch(setUsers(data.items));\r\n                dispatch(setUserCount(data.totalCount));\r\n        });\r\n    }\r\n}\r\n\r\nexport const changeSelectedPageThunkCreator = (page,pageSize) => {\r\n    return (dispatch) => {\r\n        dispatch(toogleIsFetching(true));\r\n        dispatch(selectPage(page));\r\n        userAPI.getUsersBySize(page,pageSize).then(data => {\r\n            dispatch(setUsers(data.items));\r\n            dispatch(toogleIsFetching(false));\r\n        });\r\n    }\r\n}\r\n\r\nexport const followThunkCreator = (userId) => {\r\n    return (dispatch) => {\r\n        dispatch(toogleFollowingInProgress(true,userId));\r\n        userAPI.follow(userId).then(response => {\r\n                if(response.data.resultCode == 0){\r\n                    dispatch(follow(userId));\r\n                }\r\n                dispatch(toogleFollowingInProgress(false,userId));\r\n            }); \r\n    }\r\n}\r\n\r\nexport const unfollowThunkCreator = (userId) => {\r\n    return (dispatch) => {\r\n        dispatch(toogleFollowingInProgress(true,userId));\r\n        u